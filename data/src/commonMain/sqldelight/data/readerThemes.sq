CREATE TABLE
IF NOT EXISTS `reader_theme`
(`_id` INTEGER NOT NULL PRIMARY KEY,
`background_color` INTEGER AS Int NOT NULL,
`on_textcolor` INTEGER AS Int NOT NULL);


